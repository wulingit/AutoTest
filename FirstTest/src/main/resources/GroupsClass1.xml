            <run>
                <include name="stu"/>
            </run>
        </groups>
        <classes>
            <class name="com.first.testng.groups.GroupsClass1"/>
            <class name="com.first.testng.groups.GroupsClass2"/>
            <class name="com.first.testng.groups.GroupsClass3"/>
        </classes>
    </test>
</suite>